<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kieran.app.address.view.EmailController">
   <children>
      <Label layoutX="36.0" layoutY="14.0" text="Send Email Notification" />
      <ButtonBar layoutX="320.0" layoutY="144.0" prefHeight="40.0" prefWidth="167.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleSend" text="Send" />
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
      <TextArea fx:id="emailTxt" layoutX="26.0" layoutY="78.0" prefHeight="69.0" prefWidth="455.0" promptText="Please enter your text" />
      <TextField fx:id="emailHeader" layoutX="26.0" layoutY="43.0" prefHeight="25.0" prefWidth="455.0" promptText="Email Header" />
   </children>
</AnchorPane>
